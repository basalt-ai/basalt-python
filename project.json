{
	"name": "py-sdk",
	"projectType": "library",
	"sourceRoot": "packages/py-sdk",
    "targets": {
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"commands": [
					"cd packages/py-sdk && python -m build"
				]
			}
		},

		"publish": {
			"executor": "nx:run-commands",
			"options": {
				"commands": [
					"cd packages/py-sdk && twine upload dist/*"
				]
			}
		}
	}
}
